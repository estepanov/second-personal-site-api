{
  "name": "personal-site-api",
  "version": "1.0.0",
  "description": "Serverless webpack example using Typescript",
  "main": "handler.js",
  "scripts": {
    "dev": "sls offline --stage dev",
    "deploy:dev": "sls deploy --stage dev --aws-profile serverless-deploy",
    "deploy:prod": "sls deploy --stage production --aws-profile serverless-deploy",
    "lint": "eslint . --ext js,ts --cache --fix",
    "prettier": "prettier --list-different './**/*.{js,ts}'",
    "typecheck": "tsc --noEmit",
    "buildtest": "tsc --noEmit",
    "test": "NODE_ENV=test ./node_modules/.bin/jest --ci --verbose"
  },
  "dependencies": {
    "@dazn/lambda-powertools-cloudwatchevents-client": "^1.24.1",
    "@dazn/lambda-powertools-correlation-ids": "^1.24.1",
    "@dazn/lambda-powertools-dynamodb-client": "^1.24.1",
    "@dazn/lambda-powertools-kinesis-client": "^1.24.1",
    "@dazn/lambda-powertools-lambda-client": "^1.24.1",
    "@dazn/lambda-powertools-logger": "^1.24.1",
    "@dazn/lambda-powertools-pattern-basic": "^1.25.0",
    "@dazn/lambda-powertools-sns-client": "^1.24.1",
    "@dazn/lambda-powertools-sqs-client": "^1.24.1",
    "@hapi/address": "^4.1.0",
    "@upstash/redis": "^0.2.1",
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.814.0",
    "disposable-email-domains": "^1.0.56",
    "hcaptcha": "^0.0.2",
